{
  "name": "lenny-transcript-api",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "node index.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "puppeteer": "^24.10.2",
    "puppeteer-core": "^24.10.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "vercel": {
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }
}
